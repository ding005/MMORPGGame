ChapterEntity = { Id = 0, ChapterName = "", GameLevelCount = 0, BG_Pic = "", BranchLevelId = {}, BranchLevelName = {}, Uvx = 0, Uvy = 0 }

ChapterEntity.__index = ChapterEntity;

function ChapterEntity.New(Id, ChapterName, GameLevelCount, BG_Pic, BranchLevelId, BranchLevelName, Uvx, Uvy)
    local self = { };
    setmetatable(self, ChapterEntity);

    self.Id = Id;
    self.ChapterName = ChapterName;
    self.GameLevelCount = GameLevelCount;
    self.BG_Pic = BG_Pic;
    self.BranchLevelId = BranchLevelId;
    self.BranchLevelName = BranchLevelName;
    self.Uvx = Uvx;
    self.Uvy = Uvy;

    return self;
end

function ChapterEntity.NewFromArrItem(item)
    local self = { };
    setmetatable(self, ChapterEntity);

    self.Id = item[1];
    self.ChapterName = item[2];
    self.GameLevelCount = item[3];
    self.BG_Pic = item[4];
    self.BranchLevelId = item[5];
    self.BranchLevelName = item[6];
    self.Uvx = item[7];
    self.Uvy = item[8];

    return self;
end

